# Contributor: moostik <mooostik at gmail dot com>

pkgname=pysycache-lang-lt
pkgver=3.1b
pkgrel=1
pkgdesc="Lituanian language pack for PySyCache"
arch=('any')
url="http://www.pysycache.org/"
license=("GPL")
depends=('pysycache')
source=(http://download.tuxfamily.org/py4childs/themes/themes-move/pack-lang-gpl-lt-${pkgver}.zip)
md5sums=('2d961f8e50e1940eeb008dfd84c1825a')

build() {
	mkdir -p $startdir/pkg/usr/share/pysycache/themes-move
	cp -R $startdir/src/themes-move/* $startdir/pkg/usr/share/pysycache/themes-move
	chgrp -R pysycache $startdir/pkg/usr/share/pysycache
}
